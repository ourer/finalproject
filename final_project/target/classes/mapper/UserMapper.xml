<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.finalpj.repository.UserDAO">
	<select id="getUser" resultType="com.ezen.finalpj.domain.UserVO">
		select * from user where email=#{email}
	</select>
	
	<select id="selectList" resultType="com.ezen.finalpj.domain.UserVO">
		select name,area,age,gender from user
	</select>
</mapper>